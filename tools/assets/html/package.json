{
    "name": "wifi",
    "title": "wifi",
    "displayName": "wifi",
    "productName": "wifi",
    "description": "wifi",
    "publisher": "GEEKROS",
    "version": "0.1.0",
    "author": "MakerYang <www.makeryang.com>",
    "license": "MIT",
    "type": "module",
    "main": "release/dist/main/index.cjs",
    "scripts": {
        "dev": "electron-rebuild -f -w && rimraf release && cross-env SCRIPT_MODE=dev node --max-old-space-size=6096 tools/index.mjs",
        "build": "electron-rebuild -f -w && rimraf release && cross-env SCRIPT_MODE=build node --max-old-space-size=6096 tools/index.mjs",
        "win64": "electron-builder --win --x64",
        "win32": "electron-builder --win --ia32",
        "mac": "electron-builder --mac --x64",
        "linux": "electron-builder --linux --amd64",
        "postinstall": "electron-builder install-app-deps"
    },
    "env": {
        "DEV_SERVER_HOST": "0.0.0.0",
        "DEV_SERVER_PORT": 6060,
        "DEV_BACKEND_HOST": "https://www.geekros.com",
        "LOCAL_STORAGE_PREFIX": "template",
        "DESKTOP_USERAGENT": "Mozilla/5.0 ($platform) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/132.0.0.0 Safari/537.36 MicroMessenger/1.0.0 Chromiums/$version",
        "DESKTOP_NETWORK_PAC_URL": "",
        "DESKTOP_NETWORK_FILE_NAME": ""
    },
    "engines": {
        "vscode": "^1.102.0"
    },
    "categories": [
        "Other"
    ],
    "icon": "release/dist/icons/icon.png",
    "homepage": "https://www.geekros.com",
    "bugs": {
        "url": "https://github.com/geekros/studio/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/geekros/studio"
    },
    "keywords": [],
    "activationEvents": [],
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "geekstudio",
                    "title": "GeekStudio",
                    "icon": "renderer/public/icons/icon.svg"
                }
            ]
        },
        "views": {
            "geekstudio": [
                {
                    "type": "webview",
                    "id": "geekstudio.sidebar",
                    "name": "GeekStudio",
                    "icon": "renderer/public/icons/icon.svg"
                }
            ]
        },
        "commands": [
            {
                "command": "geekstudio:start",
                "title": "geekstudio:start",
                "shortTitle": "111",
                "category": "GEEKROS"
            }
        ]
    },
    "build": {
        "appId": "com.geekros.playground",
        "productName": "playground",
        "asar": true,
        "buildDependenciesFromSource": true,
        "directories": {
            "output": "release/${version}"
        },
        "fileAssociations": [],
        "publish": [
            {
                "provider": "generic",
                "url": ""
            }
        ],
        "files": [
            "release/dist"
        ],
        "dmg": {
            "contents": [
                {
                    "x": 410,
                    "y": 150,
                    "type": "link",
                    "path": "/Applications"
                },
                {
                    "x": 130,
                    "y": 150,
                    "type": "file"
                }
            ]
        },
        "mac": {
            "icon": "renderer/public/icons/macos.icns",
            "artifactName": "${productName}_Setup_${version}.${ext}"
        },
        "win": {
            "icon": "renderer/public/icons/windows.ico",
            "artifactName": "${productName}_Setup_${version}.${ext}",
            "target": "nsis",
            "requestedExecutionLevel": "highestAvailable"
        },
        "linux": {
            "icon": "renderer/public/icons/linux.icns",
            "target": [
                "deb"
            ],
            "artifactName": "${productName}_Setup_${version}.${ext}"
        },
        "nsis": {
            "oneClick": false,
            "perMachine": true,
            "allowElevation": true,
            "allowToChangeInstallationDirectory": true,
            "createDesktopShortcut": true,
            "createStartMenuShortcut": true,
            "installerIcon": "renderer/public/icons/windows.ico",
            "uninstallerIcon": "renderer/public/icons/windows.ico",
            "shortcutName": "Playground"
        }
    },
    "dependencies": {
        "@cloudflare/workers-types": "^4.20250919.0",
        "@fingerprintjs/fingerprintjs": "^4.6.2",
        "@hookform/resolvers": "^5.2.2",
        "@radix-ui/react-accordion": "^1.2.12",
        "@radix-ui/react-alert-dialog": "^1.1.15",
        "@radix-ui/react-avatar": "^1.1.10",
        "@radix-ui/react-collapsible": "^1.1.12",
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-dropdown-menu": "^2.1.15",
        "@radix-ui/react-icons": "^1.3.2",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-navigation-menu": "^1.2.14",
        "@radix-ui/react-popover": "^1.1.15",
        "@radix-ui/react-scroll-area": "^1.2.9",
        "@radix-ui/react-select": "^2.2.5",
        "@radix-ui/react-separator": "^1.1.7",
        "@radix-ui/react-slider": "^1.3.6",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-tabs": "^1.1.13",
        "@radix-ui/react-tooltip": "^1.2.8",
        "@remixicon/react": "^4.6.0",
        "@tailwindcss/typography": "^0.5.16",
        "@tailwindcss/vite": "^4.1.11",
        "@tanstack/react-table": "^8.21.3",
        "axios": "^1.10.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cmdk": "^1.1.1",
        "electron-debug": "^3.2.0",
        "electron-devtools-installer": "^4.0.0",
        "electron-localstorage": "^1.0.5",
        "electron-store": "^8.2.0",
        "electron-updater": "^6.3.9",
        "framer-motion": "^12.23.12",
        "i18next": "^25.3.2",
        "i18next-http-backend": "^3.0.2",
        "lucide-react": "^0.544.0",
        "next-themes": "^0.4.6",
        "postcss-preset-env": "^10.4.0",
        "prism-react-editor": "^1.1.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-hook-form": "^7.63.0",
        "react-i18next": "^15.6.0",
        "react-resizable-panels": "^3.0.6",
        "react-router-dom": "^7.6.3",
        "recharts": "^3.2.1",
        "request": "^2.88.2",
        "simple-icons": "^15.16.0",
        "sonner": "^2.0.7",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^4.1.11",
        "tailwindcss-animate": "^1.0.7",
        "tw-animate-css": "^1.3.5",
        "uuid": "^11.1.0",
        "vaul": "^1.1.2",
        "zod": "^4.1.11"
    },
    "devDependencies": {
        "@types/node": "^24.0.13",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.8",
        "@types/vscode": "^1.102.0",
        "@types/vscode-webview": "^1.57.5",
        "@types/yargs": "^17.0.33",
        "@vitejs/plugin-react": "^4.6.0",
        "chalk": "^5.4.1",
        "cross-env": "^7.0.3",
        "electron": "34.4.1",
        "electron-builder": "^25.1.8",
        "electron-rebuild": "^3.2.9",
        "globals": "^16.3.0",
        "rimraf": "^6.0.1",
        "rollup-plugin-visualizer": "^6.0.5",
        "terser": "^5.44.1",
        "typescript": "~5.8.3",
        "vite": "^7.0.4",
        "vite-plugin-chunk-split": "^0.5.0",
        "yargs": "^18.0.0"
    }
}
